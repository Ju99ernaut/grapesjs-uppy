        "@uppy/dropbox": "^1.5.2",
        "@uppy/facebook": "^1.2.2",
        "@uppy/google-drive": "^1.7.2",
        "@uppy/instagram": "^1.5.2",
        "@uppy/onedrive": "^1.2.2",
        "@uppy/screen-capture": "^1.1.0",
        "@uppy/tus": "^1.9.2",
        "@uppy/unsplash": "^0.1.13",
        "@uppy/webcam": "^1.8.13"
    },
    "author": "Brendon Ngirazi"
