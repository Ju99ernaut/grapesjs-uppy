        "@uppy/dropbox": "^1.4.20",
        "@uppy/facebook": "^1.1.20",
        "@uppy/google-drive": "^1.5.20",
        "@uppy/instagram": "^1.4.20",
        "@uppy/onedrive": "^1.1.20",
        "@uppy/screen-capture": "^1.0.11",
        "@uppy/tus": "^1.8.2",
        "@uppy/unsplash": "^0.1.4",
        "@uppy/webcam": "^1.8.2"
