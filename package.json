        "@uppy/dropbox": "^2.0.5",
        "@uppy/facebook": "^2.0.5",
        "@uppy/google-drive": "^2.0.5",
        "@uppy/instagram": "^2.0.5",
        "@uppy/onedrive": "^2.0.5",
        "@uppy/screen-capture": "^2.0.6",
        "@uppy/tus": "^2.2.0",
        "@uppy/unsplash": "^2.0.3",
        "@uppy/webcam": "^2.0.5"
    },
    "author": "Brendon Ngirazi"
