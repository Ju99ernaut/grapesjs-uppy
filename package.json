{
    "name": "grapesjs-uppy",
    "version": "2.0.0",
    "description": "Grapesjs Uppy",
    "main": "dist/grapesjs-uppy.min.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ju99ernaut/grapesjs-uppy.git"
    },
    "scripts": {
        "start": "grapesjs-cli serve",
